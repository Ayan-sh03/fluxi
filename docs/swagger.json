                        "schema": {
                            "$ref": "#/definitions/api.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/api-keys": {
            "get": {
                "security": [
                    {
                        "ApiKeyAuth": []
                    }
                ],
                "description": "Retrieves all API keys for the user",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
